{
  "name": "pantry-to-plate",
  "private": true,
  "version": "1.0.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently -n client,server -c auto,auto \"pnpm --filter @pantry/client dev\" \"pnpm --filter @pantry/server dev\"",
    "dev:client": "pnpm --filter @pantry/client dev",
    "dev:server": "pnpm --filter @pantry/server dev",
    "typecheck": "tsc -b",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier . --write"
  },
  "devDependencies": {
    "concurrently": "^9.0.0",
    "eslint": "^9.13.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.19.0"
}
